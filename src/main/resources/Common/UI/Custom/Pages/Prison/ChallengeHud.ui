    LayoutMode: Top;
    Visible: false;
    Label #Name {
      Anchor: (Height: 16);
      Text: "";
      Style: (FontSize: 11, TextColor: #ffd700, RenderBold: true);
    }
    Label #Bar {
      Anchor: (Height: 14);
      Text: "";
      Style: (FontSize: 10, TextColor: #96a9be);
    }
    Group {
      Anchor: (Height: 14);
      LayoutMode: Left;
